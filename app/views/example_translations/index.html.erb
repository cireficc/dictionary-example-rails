<p id="notice"><%= notice %></p>

<h1>Example Translations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @example_translations.each do |example_translation| %>
      <tr>
        <td><%= link_to 'Show', example_translation %></td>
        <td><%= link_to 'Edit', edit_example_translation_path(example_translation) %></td>
        <td><%= link_to 'Destroy', example_translation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Example Translation', new_example_translation_path %>
